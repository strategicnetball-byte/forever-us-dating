{
  "name": "forever-us-dating",
  "version": "1.0.0",
  "description": "Forever Us - A modern dating application",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client-new && npm start",
    "build": "cd client-new && npm run build",
    "install-all": "npm install && cd client-new && npm install",
    "seed": "node server/scripts/seedProfiles.js",
    "add-photos": "node server/scripts/addDummyPhotos.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^7.0.0",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  }
}
